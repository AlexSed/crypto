    "@types/request": "^0.0.42",
    "coveralls": "^2.13.0",
    "eslint": "^3.19.0",
    "nyc": "^10.2.0",
    "nan": "^2.6.1",
    "tslint": "^5.1.0",
    "typescript": "^2.2.2",
    "mocha": "^3.2.0"
